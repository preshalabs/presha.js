{
  "name": "presha",
  "version": "0.0.2",
  "description": "A minimal node.js framework built for clean architecture and low-pressure development.",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "npx prettier . --write"
  },
  "bin": {
    "presha": "dist/cli/index.js"
  },
  "main": "dist/index.js",
  "type": "module",
  "exports": {
    ".": "./dist/index.js",
    "./runtime/express": {
      "types": "./dist/runtime/express.d.ts",
      "import": "./dist/runtime/express.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/preshalabs/presha.js.git"
  },
  "author": "preshalabs",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/preshalabs/presha.js/issues"
  },
  "homepage": "https://github.com/preshalabs/presha.js#readme",
  "dependencies": {
    "commander": "^14.0.0",
    "express": "^5.1.0",
    "ts-morph": "^26.0.0",
    "typescript": "^5.8.3",
    "zod": "^3.25.62"
  },
  "devDependencies": {
    "@eslint/js": "^9.28.0",
    "@types/express": "^5.0.3",
    "@types/node": "^22.15.29",
    "chokidar": "^4.0.3",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.1",
    "globals": "^16.2.0",
    "prettier": "3.5.3",
    "tsup": "^8.5.0",
    "typescript-eslint": "^8.33.1"
  }
}
